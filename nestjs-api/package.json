{
  "name": "nestjs-api",
  "version": "0.0.1",
  "description": "",
  "author": "coeps",
  "license": "MIT",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch --watchAssets",
    "start:hmr": "nest build --webpack --webpackPath webpack-hmr.config.js --watchAssets",
    "start:debug": "nest start --debug --watch --watchAssets",
    "start:prod": "node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json"
  },
  "dependencies": {
    "@nestjs/common": "7.4.4",
    "@nestjs/config": "0.5.0",
    "@nestjs/core": "7.4.4",
    "@nestjs/jwt": "7.1.0",
    "@nestjs/passport": "7.1.0",
    "@nestjs/platform-express": "7.4.4",
    "@nestjs/platform-ws": "7.4.4",
    "@nestjs/serve-static": "2.1.3",
    "@nestjs/swagger": "4.6.1",
    "@nestjs/terminus": "7.0.1",
    "@nestjs/websockets": "7.4.4",
    "cookie-parser": "1.4.5",
    "csurf": "1.11.0",
    "express-rate-limit": "5.1.3",
    "express-winston": "4.0.5",
    "helmet": "4.1.1",
    "nest-winston": "1.4.0",
    "passport": "0.4.1",
    "passport-jwt": "4.0.0",
    "passport-local": "1.0.0",
    "reflect-metadata": "0.1.13",
    "rimraf": "3.0.2",
    "rxjs": "6.6.3",
    "swagger-ui-express": "4.1.4",
    "winston": "3.3.3",
    "ws": "7.3.1"
  },
  "devDependencies": {
    "@nestjs/cli": "7.5.1",
    "@nestjs/schematics": "7.1.2",
    "@nestjs/testing": "7.4.4",
    "@types/express": "4.17.8",
    "@types/jest": "26.0.14",
    "@types/node": "13.13.23",
    "@types/passport-jwt": "3.0.3",
    "@types/passport-local": "1.0.33",
    "@types/supertest": "2.0.10",
    "@types/ws": "7.2.7",
    "@typescript-eslint/eslint-plugin": "3.10.1",
    "@typescript-eslint/parser": "3.10.1",
    "eslint": "7.10.0",
    "eslint-config-prettier": "6.12.0",
    "eslint-plugin-import": "2.22.1",
    "jest": "26.5.2",
    "prettier": "1.19.1",
    "start-server-webpack-plugin": "2.2.5",
    "supertest": "4.0.2",
    "ts-jest": "26.4.1",
    "ts-loader": "6.2.2",
    "ts-node": "9.0.0",
    "tsconfig-paths": "3.9.0",
    "typescript": "3.9.7",
    "webpack-node-externals": "2.5.2"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".spec.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
